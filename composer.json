{
    "name": "walkingdexter/phpcs-diff",
    "description": "Removes non-diff violations from PHP_CodeSniffer reports.",
    "type": "library",
    "keywords": [
        "phpcs",
        "diff"
    ],
    "homepage": "https://github.com/WalkingDexter/phpcs-diff",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrey Tymchuk"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "bin": [
        "bin/phpcs-diff"
    ]
}
